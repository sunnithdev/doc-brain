{
  "name": "doc-brain",
  "version": "1.0.0",
  "description": "> “Chat with your documents, powered by cloud AI”",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "tsc src/main/main.ts src/main/preload.ts --outDir dist/main --watch --esModuleInterop --module CommonJS --target ES2022 --skipLibCheck",
    "build": "tsc src/main/main.ts src/main/preload.ts --outDir dist/main --esModuleInterop --module CommonJS --target ES2022 --skipLibCheck && vite build",
    "start": "NODE_ENV=development electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunnithdev/doc-brain.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sunnithdev/doc-brain/issues"
  },
  "homepage": "https://github.com/sunnithdev/doc-brain#readme",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.8",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  },
  "dependencies": {
    "@langchain/community": "^1.0.7",
    "@langchain/core": "^1.1.4",
    "@langchain/openai": "^1.1.3",
    "@langchain/textsplitters": "^1.0.1",
    "dotenv": "^16.6.1",
    "hnswlib-node": "^3.0.0",
    "langchain": "^1.1.5",
    "pdf-parse": "^1.1.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "zustand": "^5.0.9"
  }
}
